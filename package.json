{
  "name": "nwc-viewer",
  "version": "1.0.0",
  "description": "Viewer for Noteworthy Composer files",
  "main": "index.js",
  "scripts": {
    "pretty": "prettier --single-quote --use-tabs --no-semi --trailing-comma es5 --write src/*.js",
    "test": "node -r esm test/test_nwc.js",
    "lint": "eslint src/*.js",
    "build": "rollup src/main.js --output.format iife --output.file bin/notably.js"
  },
  "author": "Joshua Koo",
  "bugs": {
    "url": "https://github.com/zz85/nwc-viewer/issues"
  },
  "homepage": "https://github.com/zz85/nwc-viewer",
  "license": "GNU",
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.4.0",
    "esm": "^3.2.25",
    "prettier": "^1.9.2",
    "rollup": "^0.53.1"
  }
}
